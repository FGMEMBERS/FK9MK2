<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>inttour</object-name>
    <object-name>planchet</object-name>
    <object-name>siege</object-name>

    <!-- Objets transparents -->
    <object-name>intvitres</object-name>
  </animation>

  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <object-name>intvitres</object-name>
  </effect>
  
  <!-- Pilote -->
  <model>
    <path>Aircraft/FK9MK2/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -1.310 </x-m>
      <y-m> -0.206 </y-m>
      <z-m> -0.220 </z-m>
    </offsets>
  </model>

  <!-- Tableau de bord -->
  <model>
    <path>Aircraft/FK9MK2/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -1.151 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.157 </z-m>
    </offsets>
  </model>

</PropertyList>
