<?xml version="1.0"?>

<PropertyList>

  <path>interior.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>interieur</object-name>
    <object-name>siege</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- Pilote -->
  <model>
    <path>Aircraft/FK9MK2/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -1.310 </x-m>
      <y-m> -0.206 </y-m>
      <z-m> -0.220 </z-m>
    </offsets>
  </model>

  <!-- Tableau de bord -->
  <model>
    <path>Aircraft/FK9MK2/Models/Interior/Panel/panel.xml</path>
    <offsets>
      <x-m> -1.151 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.157 </z-m>
      <roll-deg>    0.000 </roll-deg>
      <pitch-deg>   0.000 </pitch-deg>
      <heading-deg> 0.000 </heading-deg>
    </offsets>
  </model>

</PropertyList>

